<#ftl encoding="utf-8" />
<#import "/share/freemarker/funnelback_legacy.ftl" as s/>

<!DOCTYPE HTML PUBLIC "-//W3C//DTD HTML 4.01//EN"
                      "http://www.w3.org/TR/html4/strict.dtd">
<html lang="en">
<head>
	<title>
		<@s.AfterSearchOnly><@s.cgi>query</@s.cgi><@s.cgi>query_prox</@s.cgi><@s.IfDefCGI name="query">,</@s.IfDefCGI></@s.AfterSearchOnly>
        	<@s.cfg>service_name</@s.cfg>, Funnelback Search
	</title>

	<@s.OpenSearch></@s.OpenSearch>
	<meta name="robots" content="nofollow">
	<meta http-equiv="Content-Type" content="text/html;charset=utf-8">	
        <link rel="stylesheet" media="screen" href="/search/search.css" type="text/css">
	<!--[if !IE]>-->
        <link rel="stylesheet" media="handheld, only screen and (max-device-width: 480px)" href="/search/search-mobile.css" type="text/css">
	<!--<![endif]-->
	<meta name="viewport" content="width=320; initial-scale=1.0; maximum-scale=1.0; user-scalable=1;">

	<!-- Query completion -->
	<script type="text/javascript" src="/search/js/jquery/jquery-1.4.2.min.js"></script>
	<script type="text/javascript" src="/search/js/jquery/jquery-ui-1.8.2.custom.min.js"></script>
	<script type="text/javascript" src="/search/js/jquery/jquery.tmpl.min.js"></script>
	<script type="text/javascript" src="/search/js/jquery.funnelback-completion.js"></script>

	<script type="text/javascript">
                // Query completion setup.
                jQuery(function() {
                    jQuery("#query").fbcompletion({
                        'enabled'    : '<@s.cfg>query_completion</@s.cfg>',
                        'collection' : '<@s.cfg>collection</@s.cfg>',
                        'program'    : '<@s.cfg>query_completion.program</@s.cfg>',
                        <#if SearchTransaction.question.collection.configuration.value("query_completion.source")?exists && SearchTransaction.question.collection.configuration.value("query_completion.source") != "internal">'source'     : 'external',</#if>
                        'format'     : '<@s.cfg>query_completion.format</@s.cfg>',
                        'alpha'      : '<@s.cfg>query_completion.alpha</@s.cfg>',
                        'show'       : '<@s.cfg>query_completion.show</@s.cfg>',
                        'sort'       : '<@s.cfg>query_completion.sort</@s.cfg>', 
                        'length'     : '<@s.cfg>query_completion.length</@s.cfg>',
                        'delay'      : '<@s.cfg>query_completion.delay</@s.cfg>'
                    });
                });

		// Advanced search toggle.
		window.onload = function () {
			if (document.getElementById('fb-advanced-toggle') ) {
				document.getElementById('fb-advanced-toggle').onclick = function() {
					advancedSearch = document.getElementById('fb-advanced');
					simpleSearch = document.getElementById('fb-queryform');
					this.innerHTML = (this.innerHTML != 'Advanced search' ? 'Advanced search' : 'Simple search');
					advancedSearch.style.display = (advancedSearch.style.display != 'none' ? 'none' : '');
					simpleSearch.style.display = (simpleSearch.style.display != 'none' ? 'none' : '');
					return false;
				}
			}
			if (document.getElementById('fb-advanced-view') ) {
				document.getElementById('fb-advanced-view').onclick = function() {
					advancedSearch = document.getElementById('fb-advanced');
					simpleSearch = document.getElementById('fb-queryform');
					document.getElementById('fb-advanced-toggle').innerHTML = (document.getElementById('fb-advanced-toggle').innerHTML != 'Advanced search' ? 'Advanced search' : 'Simple search');
					advancedSearch.style.display = (advancedSearch.style.display != 'none' ? 'none' : '');
					simpleSearch.style.display = (simpleSearch.style.display != 'none' ? 'none' : '');
					return false;
				}
			}
		};
	</script>
</head>
<body>

	<!-- NO QUERY SPECIFIED -->
	<@s.InitialFormOnly>
		<div id="fb-initial">
			<a href="http://funnelback.com/"><img src="/search/funnelback.png" alt="Funnelback logo"></a>
		</div>
	</@s.InitialFormOnly>


	<!-- SEARCH LOGO -->	
	<div><a id="fb-logo" <@s.InitialFormOnly>class="fb-initial"</@s.InitialFormOnly> href="http://funnelback.com/"><img src="/search/funnelback-small.png" alt="Funnelback logo" width="170" height="36"></a></div>


	<!-- QUERY FORM -->
	<div id="fb-queryform" <@s.InitialFormOnly>class="fb-initial"</@s.InitialFormOnly> <@s.IfDefCGI name="fb-advanced"> style="display: none;"</@s.IfDefCGI>>
		<form action="" method="GET">
			<div>
			<label for="query" style="text-indent: -9999em;">Search</label>
			<input name="query" id="query" type="text" value="<@s.cgi>query</@s.cgi>" accesskey="q">
			<input type="submit" value="Search">
			<input type="hidden" name="collection" value="<@s.cgi>collection</@s.cgi>">
			<@s.IfDefCGI name="enc"><input type="hidden" name="enc" value="<@s.cgi>enc</@s.cgi>"></@s.IfDefCGI>
			<@s.IfDefCGI name="form"><input type="hidden" name="form" value="<@s.cgi>form</@s.cgi>"></@s.IfDefCGI>
			<@s.IfDefCGI name="scope"><input type="hidden" name="scope" value="<@s.cgi>scope</@s.cgi>"></@s.IfDefCGI>
			<@s.IfDefCGI name="profile"><input type="hidden" name="profile" value="<@s.cgi>profile</@s.cgi>"></@s.IfDefCGI>
			<s:FacetScope>Within selected categories only</s:FacetScope>
                        <@s.IfDefCGI name="from-advanced"><br /><span id="fb-advanced-note">Your search contained advanced operators [<a href="?fb-advanced=true&amp;${SearchTransaction.question.queryString?html}" id="fb-advanced-view">view advanced search</a>]</span></@s.IfDefCGI>
			</div>
        	</form>
	</div> <!-- #fb-queryform -->

	<!-- ADVANCED SEARCH FORM (HIDDEN BY DEFAULT) -->
	<div id="fb-advanced"<@s.IfNotDefCGI name="fb-advanced"> style="display: none;"</@s.IfNotDefCGI>>
		<form action="" method="GET">
			<div>
                        <input type="hidden" name="from-advanced" value="true">
			<input type="hidden" name="collection" value="<@s.cgi>collection</@s.cgi>">
			<@s.IfDefCGI name="enc"><input type="hidden" name="enc" value="<@s.cgi>enc</@s.cgi>"></@s.IfDefCGI>
			<@s.IfDefCGI name="form"><input type="hidden" name="form" value="<@s.cgi>form</@s.cgi>"></@s.IfDefCGI>
			<@s.IfDefCGI name="scope"><input type="hidden" name="scope" value="<@s.cgi>scope</@s.cgi>"></@s.IfDefCGI>
			<@s.IfDefCGI name="profile"><input type="hidden" name="profile" value="<@s.cgi>profile</@s.cgi>"></@s.IfDefCGI>

			<label for="query-advanced" style="text-indent: -9999em;">Advanced Search</label>
			<input name="query" id="query-advanced" type="text" value="<@s.cgi>query</@s.cgi>" accesskey="q">

			<fieldset>
				<legend>Results that have...</legend>
				<span><label for="query_and">All the words</label>
			        <input type="text" id="query_and" name="query_and" value="<@s.cgi>query_and</@s.cgi>"></span>

				<span><label for="query_phrase">The phrase</label>
			        <input type="text" id="query_phrase" name="query_phrase" value="<@s.cgi>query_phrase</@s.cgi>"></span>

				<span><label for="query_not">None of the words</label>
				<input type="text" id="query_not" name="query_not" value="<@s.cgi>query_not</@s.cgi>"></span>
			</fieldset>

			<fieldset>
				<legend>With metadata of...</legend>
				<span><label for="meta_t">Title</label>
				<input type="text" id="meta_t" name="meta_t" value="<@s.cgi>meta_t</@s.cgi>"></span>

				<span><label for="meta_a">Author</label>
				<input type="text" id="meta_a" name="meta_a" value="<@s.cgi>meta_a</@s.cgi>"></span>

				<span><label for="meta_s">Subject</label>
				<input type="text" id="meta_s" name="meta_s" value="<@s.cgi>meta_s</@s.cgi>"></span>


			</fieldset>

			<fieldset>
				<legend>Published...</legend>
				<span><label for="meta_d1day">Published after</label>
				<@s.Select name="meta_d1day" options=[""] range="1..31" />
				<@s.Select name="meta_d1month" options=["", "Jan", "Feb", "Mar", "Apr", "May", "Jun", "Jul", "Aug", "Sep", "Oct", "Nov", "Dec"] />
				<@s.Select name="meta_d1year" options=[""] range="CURRENT_YEAR - 20..CURRENT_YEAR" /></span>
	
				<span><label for="meta_d2day">Published before</label>
				<@s.Select name="meta_d2day" options=[""] range="1..31" />
		                <@s.Select name="meta_d2month" options=["", "Jan", "Feb", "Mar", "Apr", "May", "Jun", "Jul", "Aug", "Sep", "Oct", "Nov", "Dec"] />
				<@s.Select name="meta_d2year" options=[""] range="CURRENT_YEAR - 20..CURRENT_YEAR + 1" /></span>

				<span><label for="scope">Within (e.g. example.com)</label>
				<input type="text" id="scope" name="scope" value="<@s.cgi>scope</@s.cgi>"></span>
			</fieldset>

			<div>
				<span><label for="meta_f_sand">File type</label>
				<@s.Select name="meta_f_sand" options=["=Any ", "pdf=PDF  (.pdf) ", "xls=Excel (.xls) ", "ppt=Powerpoint (.ppt) ", "rtf=Rich Text (.rtf) ", "doc=Word (.doc) ", "docx=Word 2007+ (.docx) "] /></span>

				<span><label class="fb-sortby" for="sort">Sort results by</label>
				<@s.Select name="sort" options=["=Relevance ", "date=Date", "title=Title", "url=URL"] /></span>


				<input type="submit" value="Search" id="fb-advanced-submit">
			</div>

			</div>
		</form>
	</div> <!-- #fb-advanced -->


	<!-- SEARCH -->
	<@s.AfterSearchOnly>

	<p id="fb-matching" class="<@s.FacetedSearch>fb-with-faceting</@s.FacetedSearch>">
		<span id="fb-utils">
			<@s.IfNotDefCGI name="fb-advanced">
				<a href="?fb-advanced=true&amp;${SearchTransaction.question.queryString?html}" id="fb-advanced-toggle">Advanced search</a>
			</@s.IfNotDefCGI>
			<@s.IfDefCGI name="fb-advanced">
				<a href="?collection=<@s.cgi>collection</@s.cgi>&amp;form=<@s.cgi>form</@s.cgi>&amp;query=<@s.cgi>query</@s.cgi>" id="fb-advanced-toggle">Simple search</a>
			</@s.IfDefCGI>

			<span id="fb-help">| <a href="/search/help/simple_search.html">Help</a></span>
		</span>

		<#if SearchTransaction.response.resultPacket.resultsSummary.totalMatching == 0>
			<span class="fb-result-count" id="fb-total-matching">0</span> search results for <strong>${SearchTransaction.response.resultPacket.queryCleaned}</strong>
		</#if>
		<#if SearchTransaction.response.resultPacket.resultsSummary.totalMatching != 0>
			<span class="fb-result-count" id="fb-page-start">${SearchTransaction.response.resultPacket.resultsSummary.currStart}</span> -
			<span class="fb-result-count" id="fb-page-end">${SearchTransaction.response.resultPacket.resultsSummary.currEnd}</span> of
			<span class="fb-result-count" id="fb-total-matching">${SearchTransaction.response.resultPacket.resultsSummary.totalMatching?string.number}</span>
			search results for <strong>${SearchTransaction.response.resultPacket.queryCleaned}</strong>
		</#if>
		<#if SearchTransaction.response.resultPacket.resultsSummary.partiallyMatching != 0>
			where
			<span class="fb-result-count" id="fb-fully-matching">
				${SearchTransaction.response.resultPacket.resultsSummary.fullyMatching?string.number}
			</span>
			match all words and
			<span class="fb-result-count" id="fb-partially-matching">
				${SearchTransaction.response.resultPacket.resultsSummary.partiallyMatching?string.number}
			</span>
			match some words.
		</#if>
	</p> <!-- #fb-matching -->
	
	<div id="fb-wrapper" class="<@s.FacetedSearch>fb-with-faceting</@s.FacetedSearch>">
	
		<!-- RESULTS SUMMARY -->
		<div id="fb-summary">
			
			<!-- SEARCH SCOPE -->
			<@s.IfDefCGI name="scope">
				<p id="fb-scope">
					Search scope: <@s.Truncate length=80><@s.cgi>scope</@s.cgi></@s.Truncate>
					(<a href="?collection=<@s.cgi>collection</@s.cgi>&amp;form=<@s.cgi>form</@s.cgi>&amp;query=<s:URLEncode>${SearchTransaction.response.resultPacket.queryCleaned}</s:URLEncode>">Broaden search scope</a>)
				</p>
			</@s.IfDefCGI>
	 
			<!-- Did you mean...? -->
			<span id="fb-spelling"><@s.CheckSpelling></@s.CheckSpelling></span>
			
			<!-- TIER BARS -->
			<s:TierBarBestBets></s:TierBarBestBets>
			<s:TierBarFullyMatching></s:TierBarFullyMatching>
			<s:TierBarPartiallyMatching><h2 class="fb-title">Search results that match tier{constraints_matching} of tier{num_constraints} words</h2></s:TierBarPartiallyMatching>

		</div> <!-- #fb-summary -->
		
		<!-- FEATURED PAGES (BEST BETS) -->
		<@s.BestBets>
		<div class="fb-best-bet">
			<#if s.bb.title?exists><h3><a href="${s.bb.clickTrackingUrl}"><@s.boldicize bold=SearchTransaction.response.resultPacket.query>${s.bb.title}</@s.boldicize></a></h3></#if>
			<#if s.bb.description?exists><p><@s.boldicize bold=SearchTransaction.response.resultPacket.query>${s.bb.description}</@s.boldicize></p></#if>
			<#if s.bb.title?exists><p><cite>${s.bb.link}</cite></p></#if>
			<#if ! s.bb.title?exists><p><strong>${s.bb.trigger}:</strong> <a href="${s.bb.link}">${s.bb.link}</a></#if>
		</div>
		</@s.BestBets>

		<!-- RESULTS -->

		<ol id="fb-results">

			<!-- EACH RESULT -->
			<@s.Results>
			<li>
				<h3>
					<#if s.result.fileType == "pdf"><span class="fb-filetype fb-pdf">[PDF]</span></#if>
					<#if s.result.fileType == "doc"><span class="fb-filetype fb-doc">[DOC]</span></#if>
					<#if s.result.fileType == "xls"><span class="fb-filetype fb-xls">[XLS]</span></#if>
					<#if s.result.fileType == "ppt"><span class="fb-filetype fb-ppt">[PPT]</span></#if>
					<#if s.result.fileType == "rtf"><span class="fb-filetype fb-rtf">[RTF]</span></#if>
					<a href="${s.result.clickTrackingUrl}" title="${s.result.liveUrl}">
						<@s.Truncate length=70>${s.result.title}</@s.Truncate>
					</a>
				</h3>
				
				<#if s.result.fileType == "pdf"><p class="fb-file-format"><span>${s.result.fileSize} - PDF</span>
				<a class="fb-cached" href="${s.result.cacheUrl}" title="View '${s.result.title}' as HTML">View as HTML</a></p></#if>
				<#if s.result.fileType == "doc"><p class="fb-file-format"><span>${s.result.fileSize} - Microsoft Word</span>
				<a class="fb-cached" href="${s.result.cacheUrl}" title="View '${s.result.title}' as HTML">View as HTML</a></p></#if>
				<#if s.result.fileType == "xls"><p class="fb-file-format"><span>${s.result.fileSize} - Microsoft Excel</span>
				<a class="fb-cached" href="${s.result.cacheUrl}" title="View '${s.result.title}' as HTML">View as HTML</a></p></#if>
				<#if s.result.fileType == "ppt"><p class="fb-file-format"><span>${s.result.fileSize} - Microsoft Powerpoint</span>
				<a class="fb-cached" href="${s.result.cacheUrl}" title="View '${s.result.title}' as HTML">View as HTML</a></p></#if>
				
				<!-- QUERY BIASED SUMMARY (WITH DATE)-->
				<#if s.result.summary?exists>
					<p><#if s.result.date?date?string("d MMM yyyy") != "No Date"><span class="fb-date">${s.result.date?date?string("d MMM yyyy")}:</span></#if>
					<span class="fb-summary">${s.result.summary}</span></p>
				</#if>

				<!-- METADATA SUMMARIES -->
                		<#if s.result.metaData.c?exists><p>${s.result.metaData.c}</p></#if>
				<#if s.result.metaData.a?exists><span class="fb-meta author"><span>by </span> ${s.result.metaData.a}</span></#if>
		                <#if s.result.metaData.s?exists><span class="fb-meta keywords"><span>Keywords:</span> ${s.result.metaData.s}</span></#if>
                		<#if s.result.metaData.p?exists><span class="fb-meta publisher"><span>Publisher:</span> ${s.result.metaData.p}</span></#if>
				
				<!-- URL AND FILESIZE -->
				<p>
				<cite><@s.cut cut="http://"><s:TruncateURL 75>${s.result.displayUrl}</s:TruncateURL></@s.cut></cite>
                		<#if ! s.result.fileType?matches("\\b(pdf|doc|xls|ppt)\\b", "r")>
					- <a class="fb-cached" href="${s.result.cacheUrl}" title="Cached version of ${s.result.title} (${s.result.rank})">Cached</a>
				</#if>
				
				<!-- EXPLORE LINKS -->
				<@s.Explore></@s.Explore>
				
				</p>

				<@s.Quicklinks>
					<ul class="fb-quicklinks">
						<@s.QuickRepeat><li><a href="${s.ql.url}" title="${s.ql.text}">${s.ql.text}</a></li></@s.QuickRepeat>
					</ul>
					<#if SearchTransaction.question.collection.configuration.value("quicklinks.domain_searchbox")?exists && SearchTransaction.question.collection.configuration.value("quicklinks.domain_searchbox") == "true">
					    <#if s.result.quickLinks.domain?matches("^[^/]*/?[^/]*$ ", "r")>
						<form class="fb-quicklinks-search" action="" method="GET">
							<div>
							<input name="query" type="text">
							<input type="submit" value="Search ${s.result.quickLinks.domain}">
							<input type="hidden" name="collection" value="<@s.cgi>collection</@s.cgi>">
							<input type="hidden" name="meta_u_sand" value="${s.result.quickLinks.domain}">
							<@s.IfDefCGI name="enc"><input type="hidden" name="enc" value="<@s.cgi>enc</@s.cgi>"></@s.IfDefCGI>
							<@s.IfDefCGI name="form"><input type="hidden" name="form" value="<@s.cgi>form</@s.cgi>"></@s.IfDefCGI>
							<@s.IfDefCGI name="scope"><input type="hidden" name="scope" value="<@s.cgi>scope</@s.cgi>"></@s.IfDefCGI>
							<@s.IfDefCGI name="profile"><input type="hidden" name="profile" value="<@s.cgi>profile</@s.cgi>"></@s.IfDefCGI>
							</div>
						</form>
					    </#if>
					</#if>
				</@s.Quicklinks>

				<span class="tagging">
					<s:ResPlugin name='ResultTagLink'>tag</s:ResPlugin>
					<s:ResPlugin name='ResultTagList'></s:ResPlugin>
				</span>
			</li>
			</@s.Results>

		<#if SearchTransaction.response.resultPacket.resultsSummary.totalMatching == 0>
			<li id="fb-no-results">Your search for <strong><@s.cgi>query</@s.cgi></strong> did not return any results. <span>Please ensure that you:</span>
				<ul>
					<li>are not using any advanced search operators like + - | " etc.</li> 
					<li>expect this document to exist within the <em><@s.cfg>service_name</@s.cfg></em><@s.IfDefCGI name="scope"> and within <em><@s.Truncate length=80><@s.cgi>scope</@s.cgi></@s.Truncate></em></@s.IfDefCGI></li>
					<li>have permission to see any documents that may match your query</li>
				</ul>
			</li> 
		</#if>
			
		</ol>
			
		<!-- SEARCH: NAVIGATION -->
		<p class="fb-page-nav">
			<@s.PrevNext separator="" label=""></@s.PrevNext>
		</p>		

		<!-- FACETED NAVIGATION -->
		<@s.FacetedSearch>
		<div id="fb-facets">
		<s:Facet>
			<h3><s:FacetLabel></s:FacetLabel></h3>
			<@s.Category>
				<@s.CategoryName></@s.CategoryName>&nbsp;<span class="fb-facet-count">(<@s.CategoryCount></@s.CategoryCount>)</span>
			</@s.Category>
			<s:MoreOrLessCategories></s:MoreOrLessCategories>
		</s:Facet>
		</div>
		</@s.FacetedSearch>


		<!-- CONTEXTUAL NAVIGATION -->
		<@s.ContextualNavigation>
		<div id="fb-contextual-navigation">
			<s:ClusterNavLayout></s:ClusterNavLayout>
			<@s.NoClustersFound></@s.NoClustersFound>
			<@s.ClusterLayout>
				<h2>Have you tried?</h2>

				<@s.Category name="type">
					<div id="fb-contextual-navigation-type">
						<span class="fb-fade"></span>
						<h3>Types of <strong>${s.contextualNavigation.searchTerm}</strong></h3>
						<ul>
						<@s.Clusters><li><a href="${s.cluster.href}"> ${s.cluster.label}</a></li></@s.Clusters>
						<@s.ShowMoreClusters category="type"><li class="fb-contextual-navigation-more"><a href="${changeParam(s.category.moreLink, "type.max_clusters", "40")}">more...</a></li></@s.ShowMoreClusters>
						<@s.ShowFewerClusters category="type"></@s.ShowFewerClusters>
						</ul>
					</div>
				</@s.Category>

				<@s.Category name="topic">
					<div id="fb-contextual-navigation-topic">
						<span class="fb-fade"></span>
						<h3>Topics on <strong>${s.contextualNavigation.searchTerm}</strong></h3>
						<ul>
						<@s.Clusters><li><a href="${s.cluster.href}"> ${s.cluster.label}</a></li></@s.Clusters>
						<@s.ShowMoreClusters category="topic"><li class="fb-contextual-navigation-more"><a href="${changeParam(s.category.moreLink, "topic.max_clusters", "40")}">more...</a></li></@s.ShowMoreClusters>
						<@s.ShowFewerClusters category="topic"></@s.ShowFewerClusters>
						</ul>
					</div>
				</@s.Category>

				<@s.Category name="site">
					<div id="fb-contextual-navigation-site">
						<span class="fb-fade"></span>
						<h3><em>${s.contextualNavigation.searchTerm}</em> by site</h3>
						<ul>
						<@s.Clusters><li><a href="${s.cluster.href}"> ${s.cluster.label}</a></li></@s.Clusters>
						<@s.ShowMoreClusters category="site"><li class="fb-contextual-navigation-more"><a href="${changeParam(s.category.moreLink, "site.max_clusters", "40")}">more...</a></li></@s.ShowMoreClusters>
						<@s.ShowFewerClusters category="site"></@s.ShowFewerClusters>
						</ul>
					</div>
				</@s.Category>
			</@s.ClusterLayout>
		</div> <!-- #fb-contextual-navigation -->
		</@s.ContextualNavigation>

	</div> <!-- #fb-wrapper -->			
	</@s.AfterSearchOnly>

</body>
</html>
