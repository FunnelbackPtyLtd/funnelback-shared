<ehcache>
	<diskStore path="java.io.tmpdir" />
	<defaultCache maxElementsInMemory="100" eternal="true" overflowToDisk="false" />
	
	<!-- Cache for collection configs -->
	<cache name="localConfigRepository" maxElementsInMemory="100" />
	
	<!-- Cache for IncludeUrl form directive -->
	<cache name="IncludeUrl" eternal="true" overflowToDisk="true" />
	
</ehcache>